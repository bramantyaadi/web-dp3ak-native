RewriteRule ^kategori-([0-9]+)-(.*)$ media.php?page=kategori&id=$1 [L]
RewriteRule ^halkategori-([0-9]+)-(.*)\.html$ media.php?page=kategori&id=$1&halkategori=$2 [L]
