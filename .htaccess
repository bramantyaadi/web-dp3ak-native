RewriteRule ^hasil-poling\.html$ media.php?page=polling&id=$1 [L]
RewriteRule ^lihat-poling\.html$ media.php?page=lihatpolling&id=$1 [L]
RewriteRule ^situs-terkait$ media.php?page=situsterkait&id=$1 [L]
